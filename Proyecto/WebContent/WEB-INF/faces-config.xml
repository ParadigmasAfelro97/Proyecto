<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
	<managed-bean>
		<managed-bean-name>mantenimientoCategorias</managed-bean-name>
		<managed-bean-class>controller.MantenimientoCategorias</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<display-name>view/crearCategoria.xhtml</display-name>
		<from-view-id>/view/crearCategoria.xhtml</from-view-id>
		<navigation-case>
			<from-action>modificarCat</from-action>
			<to-view-id>/view/modificarCita.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>view/modificarCita.xhtml</display-name>
		<from-view-id>/view/modificarCita.xhtml</from-view-id>
		<navigation-case>
			<from-action>crearCat</from-action>
			<to-view-id>/view/crearCategoria.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>view/consultarCita.xhtml</display-name>
		<from-view-id>/view/consultarCita.xhtml</from-view-id>
		<navigation-case>
			<from-action>modificarCat</from-action>
			<to-view-id>/view/modificarCita.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>view/modificarCita.xhtml</display-name>
		<from-view-id>/view/modificarCita.xhtml</from-view-id>
		<navigation-case>
			<from-action>consultaCat</from-action>
			<to-view-id>/view/consultarCita.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>view/consultarCita.xhtml</display-name>
		<from-view-id>/view/consultarCita.xhtml</from-view-id>
		<navigation-case>
			<from-action>crearCat</from-action>
			<to-view-id>/view/crearCategoria.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>view/crearCategoria.xhtml</display-name>
		<from-view-id>/view/crearCategoria.xhtml</from-view-id>
		<navigation-case>
			<from-action>consultaCat</from-action>
			<to-view-id>/view/consultarCita.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

</faces-config>
